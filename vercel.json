{
  "version": 2,
  "buildCommand": "npm install && npx @nestjs/cli build && npm run migrate:prod",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ]
}